{
  "name": "menugrab-server",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "14.x"
  },
  "main": "index.js",
  "scripts": {
    "start": "node src/server.js",
    "test": "npm run lint && tap --reporter=list",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillevc/menugrab-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guillevc/menugrab-server/issues"
  },
  "homepage": "https://github.com/guillevc/menugrab-server#readme",
  "dependencies": {
    "@now-ims/fastify-firebase": "^1.1.2",
    "fastify": "^3.10.1",
    "fastify-cors": "^5.1.0",
    "fastify-env": "^2.1.0",
    "fastify-healthcheck": "^3.1.0",
    "fastify-plugin": "^3.0.0",
    "fastify-sensible": "^3.1.0",
    "fastify-swagger": "^3.5.0",
    "firebase-admin": "^9.5.0",
    "pino-pretty": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "tap": "^14.11.0"
  },
  "eslintConfig": {
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "comma-dangle": "off",
      "global-require": "off",
      "arrow-parens": "off",
      "max-len": "off",
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "enforceInMethodNames": false,
          "allowAfterThis": true
        }
      ]
    },
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    }
  }
}